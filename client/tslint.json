{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "member-access": false,
    "interface-name": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "curly": false,
    "array-type": false,
    "no-namespace": false
  }
}
